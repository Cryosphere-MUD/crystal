#: cryotel.cc:767 cryotel.cc:1928
msgid "crystal> "
msgstr "krɪstəl> "

#: cryotel.cc:1039
#, c-format
msgid "%s - Crystal"
msgstr "%s - krɪstəl"

#: cryotel.cc:1813
msgid "/// you aren't connected.\n"
msgstr "/// ju ɑnt kənɛktɪd\n"

#: cryotel.cc:1819
msgid "/// connection closed.\n"
msgstr "/// kənɛkʃn kləʊzd\n"

#: cryotel.cc:1823
msgid ""
"/// connect <host> <port>  -- connect to a given host/port\n"
"/// close                  -- close the current connection\n"
"/// quit                   -- exit\n"
"/// z                      -- suspend\n"
"/// dump <file>            -- dump the backbuffer to a file. appends.\n"
"/// log <file>             -- sets logfile as specified. appends.\n"
"/// charset <charset>      -- interprets data from the mud as given charset\n"
msgstr ""
"/// connect <həʊst> <pɔt> - kənɛkt tu ə gɪvən həʊst ɔ pɔt\n"
"/// close - kləʊz ðə kərənt kənɛkʃn\n"
"/// quit - kwɪt\n"
"/// z - sʌspɛnd\n"
"/// dump <faɪl> - dʌmp bækbʌfə tu ə faɪl. əpɛndz\n"
"/// log <faɪl> - sɛts lɒgfaɪl æz spɛsɪfaɪd. əpɛndz\n"
"/// charset <tʃɑsɛt> - ɪntəprɪts deɪtə frɒm ðə mʌd æz gɪvən "
"tsɑsɛt\n"

#: cryotel.cc:1831
msgid "/// compress               -- show MCCP compression status \n"
msgstr "/// compress - ʃəʊ MCCP kəmprɛʃn steɪtəs\n"

#: cryotel.cc:1840
#, c-format
msgid "/// wire:%li bytes data:%li bytes.\n"
msgstr "/// waɪə:%li baɪts deɪtə:%li baɪts\n"

#: cryotel.cc:1842
msgid "/// no compression\n"
msgstr "/// nəʊ kəmprɛʃ\n"

#: cryotel.cc:1867
#, c-format
msgid "/// charset '%s' selected\n"
msgstr "/// tʃɑsɛt  ˈ%sˈ sɛlɛktɪd\n"

#: cryotel.cc:1887
msgid "/// closing old logfile.\n"
msgstr "/// kləʊsɪŋ əʊld lɒgfaɪl\n"

#: cryotel.cc:1893
#, c-format
msgid "/// couldn't open '%s' for appending.\n"
msgstr "/// kʌdənt əʊpən ˈ%sˈ fɔ əpɛndɪŋ\n"

#: cryotel.cc:1895
#, c-format
msgid "/// logging to end of '%s'.\n"
msgstr "/// lɒgɪŋ tu ɛnd əv ˈ%sˈ\n"

#: cryotel.cc:1948
